   3.7317072450543127`*^9, {3.731787250915509*^9, 3.731787252257504*^9}, 
   3.731787978251439*^9, {3.73187302893012*^9, 3.731873030430956*^9}, {
   3.7318865979221487`*^9, 3.731886600382575*^9}},
 CellLabel->"In[7]:="],

