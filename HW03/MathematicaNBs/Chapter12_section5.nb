     RowBox[{"Sin", "[", 
      RowBox[{"\[Phi]", "[", "t", "]"}], "]"}]}]}], " ", "\[Equal]", " ", 
   RowBox[{"\[Gamma]", " ", 
    RowBox[{"\[Omega]0", "^", "2"}], " ", 
